<h4 class="fs-3 mt-5">Vídeos</h4>
<% for (let i = 0; i < 5; i++) { %>
<label for="video<%= i %>" class="form-label fs-4">Vídeo <%= i + 1 %></label>
<select
  class="form-select mb-1"
  aria-label="Select video"
  name="thisClass[preClassMaterial][nonEssential][video][<%= i %>]"
  id="PreC_NE_video<%= i %>"
>
  <% if (dashboard.classes[classNum]) { %> <% if
  (dashboard.classes[classNum].preClassMaterial.nonEssential.video[i]) { %>
  <option value="">Nenhum</option>
  <% for (let video of videos) { %> <% if (video.videoId ===
  dashboard.classes[classNum].preClassMaterial.nonEssential.video[i]){ %>
  <option selected value="<%= video.videoId %>"><%= video.title %></option>
  <% } else { %>
  <option value="<%= video.videoId %>"><%= video.title %></option>
  <% } %> <% } %> <% } else { %>
  <option selected value="">Nenhum</option>
  <% for (let video of videos) { %>
  <option value="<%= video.videoId %>"><%= video.title %></option>
  <% } %> <% } %> <% } else { %>
  <option selected value="">Nenhum</option>
  <% for (let video of videos) { %>
  <option value="<%= video.videoId %>"><%= video.title %></option>
  <% } %> <% } %>
</select>
<% } %>
