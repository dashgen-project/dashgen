<h1>Adicionar materiais de apoio (<%= dashboard.title %>)
</h1>

<form action="/courseDashboards/<%= dashboard._id %>/classes/<%= classNum %>?_method=PUT" method="POST" class="mb-3">

    <input type="number" name="thisClass[classNumber]" value="<%= classNum %>" class="d-none">

    <label for="title" class="form-label fs-4">Título da aula:</label>

    <% if (dashboard.classes[classNum]) { %>
    <input type="text" class="form-control mb-3" name="thisClass[title]" id="title" aria-describedby="dashboard title"
        value="<%= dashboard.classes[classNum].title %>">
    <% } else { %>
    <input type="text" class="form-control mb-3" name="thisClass[title]" id="title" aria-describedby="dashboard title">
    <% } %>

