                        <option selected value="">Nenhum</option>
                        <% for (let video of videos) { %>
                        <option value="<%= video.videoId %>">
                            <%= video.title %>
                        </option>
                        <% } %>
                        <% } %>
                    </select>
                    <% } %>
                </div>
                <div class="carousel-item px-5">
                    <h4 class="">Slides</h4>
                    <% for (let i = 0; i < 3; i++)  { %>
                    <label for="PreC_E_slide<%= i %>" class="form-label  fs-5">Slide <%= i + 1 %></label>
                    <% if (dashboard.classes[classNum]) { %>
                    <input class="form-control mb-1" type="url"
                        name="thisClass[preClassMaterial][essential][slide][<%= i %>]" id="PreC_E_slide<%= i %>"
